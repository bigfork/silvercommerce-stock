{
    "name": "silvercommerce/stock",
    "description": "Module that adds discounts (either as a code, or on a group) to SilverCommerce",
    "type": "silverstripe-vendormodule",
    "homepage": "http://github.com/silvercommerce/stock",
    "keywords": [
        "silverstripe",
        "silvercommerce",
        "commerce",
        "ecommerce",
        "stock",
        "stock management"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Morven Lewis-Everley",
            "email": "morven@ilateral.co.uk"
        },
        {
            "name": "Mark Anderson",
            "email": "mark@ilateral.co.uk"
        }
    ],
    "support": {
        "issues": "http://github.com/silvercommerce/stock/issues"
    },
    "require": {
        "silverstripe/framework": "^5",
        "silvercommerce/settings": "dev-ss5",
        "silvercommerce/catalogue-admin": "dev-ss5",
        "silvercommerce/orders-admin": "dev-ss5",
        "i-lateral/silverstripe-gridfield-addons": "dev-ss5"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/bigfork/silvercommerce-settings"
        },
        {
            "type": "vcs",
            "url": "https://github.com/bigfork/silvercommerce-catalogue-admin"
        },
        {
            "type": "vcs",
            "url": "https://github.com/bigfork/silvercommerce-orders-admin"
        },
        {
            "type": "vcs",
            "url": "https://github.com/bigfork/silverstripe-GridFieldAddOns"
        }
    ],
    "extra": {
        "expose": [
            "client"
        ]
    },
    "autoload": {
        "psr-4": {
            "SilverCommerce\\Stock\\": "src/",
            "SilverCommerce\\Stock\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
